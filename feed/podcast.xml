---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
	<channel>
		<title>Athlone Community Radio Tech Slot</title>
		<description>Posts categorized as 'ACR884'</description>
		<link>{{ site.url }}</link>
		{% for post in site.categories.ACR884 limit:10 %}
			<item>
				<title>{{ post.title }}</title>
				<description>{{ post.content | xml_escape }}</description>
				<published>{{ post.date }}</published>
				<link>{{ site.url }}{{ post.url }}</link>
			</item>
		{% endfor %}
	</channel>
</rss>
